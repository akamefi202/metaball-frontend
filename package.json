{
  "name": "metabll-dashboard-react",
  "version": "1.2.4",
  "description": "React version of MetaBall Dashboard",
  "main": "index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "reactjs",
    "metaball",
    "metaball-react",
    "dashboard",
    "dashboard-react",
    "metaball-dashboard",
    "metaball-dashboard-react"
  ],
  "author": "developer",
  "license": "MIT",
  "bugs": {},
  "scripts": {
    "start": "set PORT=5000 && react-scripts start",
    "build": "cross-env PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^43.2.0",
    "@ckeditor/ckeditor5-react": "^9.3.0",
    "@fortawesome/fontawesome-free": "6.5.1",
    "@reduxjs/toolkit": "^2.3.0",
    "ajv": "^8.16.0",
    "axios": "^1.7.7",
    "bootstrap": "4.6.2",
    "chart.js": "2.9.4",
    "classnames": "2.3.2",
    "history": "^5.3.0",
    "html-react-parser": "^5.1.18",
    "i18next": "^23.14.0",
    "moment": "^2.29.4",
    "nouislider": "15.4.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.5",
    "react-chartjs-2": "2.11.2",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.0.1",
    "react-images-upload": "^1.2.8",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "6.21.1",
    "react-scripts": "5.0.1",
    "reactstrap": "8.10.0",
    "redux-first-history": "^5.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.3.0",
    "sass": "1.69.5",
    "sweetalert2": "^11.14.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "typescript": "5.3.3"
  },
  "overrides": {
    "svgo": "3.0.2",
    "fsevents": "2.3.2",
    "chokidar": "3.5.3"
  }
}
